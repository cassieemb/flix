<%= form_with(model: user, local: true) do |f| %>
  <%= render 'shared/errors', object: user %>

  <%=f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :email %>
  <%=f.email_field :email %>

  <%=f.label :password %>
  <%=f.password_field :password %>

  <%=f.label :confirm_password %>
  <%=f.password_field :confirm_password %>

  <%=f.submit %>
<% end %>