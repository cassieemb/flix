<section class = "container">
  <h1>There are <%=pluralize(@users.count, 'user') %>.</h1>
  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= link_to(user.name, user_path(user)) %>:
        account created <%= time_ago_in_words(user.created_at)%> ago</li>
    <% end %>
  </ul>
</section>